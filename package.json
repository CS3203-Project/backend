    "@types/joi": "^17.2.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.2.0",
    "@types/pg": "^8.15.5",
    "dotenv-cli": "^10.0.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.17.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2"
  }
